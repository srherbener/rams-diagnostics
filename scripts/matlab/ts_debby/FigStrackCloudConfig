############ PLOT DATA #################
PlotData: StrackCloud01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_cloud_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_cloud_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /cloud  XsectionData/strack_cloud_<CASE>.h5 (:,2:39,3) 1 0
End
  
PlotData: StrackCloud15Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_cloud_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_cloud_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /cloud  XsectionData/strack_cloud_<CASE>.h5 (:,2:39,31) 1 0
End
  
PlotData: StrackCloud30Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_cloud_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_cloud_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /cloud  XsectionData/strack_cloud_<CASE>.h5 (:,2:39,61) 1 0
End
  
