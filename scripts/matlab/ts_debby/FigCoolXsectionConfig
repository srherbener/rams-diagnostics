############ PLOT DATA #################
# Units in files are K/30minutes. Multiply
# by 2 to convert to K/h
PlotData: LhfCoolXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhf_cool, K
  /all_ps_lhf_cool DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
PlotData: LhfCoolDiffXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_diff_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_diff_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhf_cool, K
  /all_ps_lhf_cool_diff DIAGS/storm_diff_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
