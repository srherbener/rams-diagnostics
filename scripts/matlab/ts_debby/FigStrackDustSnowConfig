############ PLOT DATA #################
PlotData: StrackDustSnow01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_snow_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_snow_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /dust_snow  XsectionData/strack_dust_snow_<CASE>.h5 (:,2:44,3) 1 0
End
  
PlotData: StrackDustSnow15Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_snow_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_snow_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /dust_snow  XsectionData/strack_dust_snow_<CASE>.h5 (:,2:44,31) 1 0
End
  
