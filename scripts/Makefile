#
# Config
#

DEST = $(HOME)/bin
SCRIPTS = $(DEST)/lp1 \
          $(DEST)/lp2 \
          $(DEST)/unpack_src_archive

#
# Targets
#

all: $(SCRIPTS)

$(DEST)/lp1: lp1
	cp lp1 $(DEST)/lp1

$(DEST)/lp2: lp2
	cp lp2 $(DEST)/lp2

$(DEST)/unpack_src_archive: unpack_src_archive
	cp unpack_src_archive $(DEST)/unpack_src_archive

