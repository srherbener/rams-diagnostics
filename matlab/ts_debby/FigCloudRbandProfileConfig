PlotData: CloudPreSalRbandLine 2
  # x data - cloud mix ratio, g/kg
  /rb_ps_cloud DIAGS/storm_profiles_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords DIAGS/storm_profiles_<CASE>.h5 @ 1e-3 0
End
  
