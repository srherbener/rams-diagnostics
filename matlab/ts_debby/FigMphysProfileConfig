############ PLOT DATA #################
# Data units are g/kg/30minutes. Multiply by 2 to convert
# to g/kg/h.
PlotData: CloudCondPreSalCoreLine 2
  # x data - cloud cond, g/kg/h
  /all_core_ps_cloud_cond DIAGS/storm_profiles_<CASE>.h5 @ 2 0
  # y data - height, km
  /z_coords DIAGS/storm_profiles_<CASE>.h5 @ 1e-3 0
End
  
PlotData: CloudCondSalCoreLine 2
  # x data - cloud cond, g/kg/h
