############ PLOT DATA #################
PlotData: StrackRain01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_rain_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_rain_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /rain  XsectionData/strack_rain_<CASE>.h5 (:,2:39,3) 1 0
End
  
PlotData: StrackRain15Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_rain_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_rain_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /rain  XsectionData/strack_rain_<CASE>.h5 (:,2:39,31) 1 0
End
  
PlotData: StrackRain30Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_rain_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_rain_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /rain  XsectionData/strack_rain_<CASE>.h5 (:,2:39,61) 1 0
End
  
