PlotData: StrackRain60Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_rain_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_rain_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /rain  XsectionData/strack_rain_<CASE>.h5 (:,2:39,121) 1 0
End
  
############ PLOT AXES #################
PlotAxes: StrackRainAxes 3
  14 2 [0.025_0.025]
  # x axis - radius
  Linear@Distance km 0 1650 linear [_] {_}
  # y axis - utc time
  Z km 0 10 linear [_] {_}
  # z axis - rain, g/kg
  r_r g@kg^-^1 0 0.5 linear [_] {_}
End

