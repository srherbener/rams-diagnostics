############ PLOT DATA #################
PlotData: StrackThetae01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_theta_e_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_theta_e_<CASE>.h5 (2:39) 1e-3 0
  # z data -  speed_t, m/s
  /theta_e  XsectionData/strack_theta_e_<CASE>.h5 (:,2:39,3) 1 0
End
  
PlotData: StrackThetae15Contour 3
  # x data - linear distance, km
