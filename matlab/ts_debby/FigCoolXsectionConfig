############ PLOT DATA #################
# latent heat/cool data is in K per 30 minutes since
# RAMS files are saved every 30 minutes. To covert to
# K/h multiply by 2.

PlotData: LhvCoolXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:36) 1e-3 0
  # z data -  lhv_cool, K/hr
  /ps_lhv_cool_wm DIAGS/storm_xsections_<CASE>.h5 (:,1:36) 2 0
End
  
PlotData: LhvCoolXsectionPreSalDiffContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:36) 1e-3 0
  # z data -  lhv_cool, K/hr
  /ps_lhv_cool_wm_diff DIAGS/storm_xsections_<CASE>.h5 (:,1:36) 2 0
End
  
PlotData: LhvCoolXsectionSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:36) 1e-3 0
  # z data -  lhv_cool, K/hr
  /s_lhv_cool_wm DIAGS/storm_xsections_<CASE>.h5 (:,1:36) 2 0
End
  
