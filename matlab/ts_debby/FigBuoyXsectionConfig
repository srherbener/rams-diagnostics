############ PLOT DATA #################
# Units in files are K/30minutes. Multiply
# by 2 to convert to K/h
PlotData: CorePosBuoyXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 (1:20) 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  buoy_pos_acc, K
  /all_core_ps_buoy_pos_acc DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 1 0
End
  
PlotData: CorePosBuoyDiffXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_diff_xsections_<CASE>.h5 (1:20) 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_diff_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  buoy_pos_acc, K
  /all_core_ps_buoy_pos_acc_diff DIAGS/storm_diff_xsections_<CASE>.h5 (:,1:44) 1 0
End
  
