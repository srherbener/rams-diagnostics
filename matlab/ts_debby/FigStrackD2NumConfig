############ PLOT DATA #################
PlotData: StrackD2Num01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_d2_num_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_d2_num_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /d2_num  XsectionData/strack_d2_num_<CASE>.h5 (:,2:44,3) 1 0
End
  
PlotData: StrackD2Num15Contour 3
  # x data - linear distance, km
