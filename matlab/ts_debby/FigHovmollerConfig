  # x data - radius, km
  /x_coords DIAGS/hist_meas_az_pcprate_<CASE>.h5 (1:34) 1e-3 0
  # y data - sim time in hours
  /t_coords DIAGS/hist_meas_az_pcprate_<CASE>.h5 @ 2.7778e-4 -42
  # z data -  precip rate, mm/h
  /pcprate_ts DIAGS/hist_meas_az_pcprate_<CASE>.h5 (1:34,:) 1 0
End
  
