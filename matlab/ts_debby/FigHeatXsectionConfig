############ PLOT DATA #################
# latent heat/cool data is in K per 30 minutes since
# RAMS files are saved every 30 minutes. To covert to
# K/h multiply by 2.

PlotData: LhvHeatXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhv_heat, K/hr
  /all_ps_lhv_heat DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
PlotData: LhvHeatXsectionPreSalDiffContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhv_heat, K/hr
  /all_ps_lhv_heat_diff DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
PlotData: LhvHeatXsectionSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhv_heat, K/hr
  /all_s_lhv_heat DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
