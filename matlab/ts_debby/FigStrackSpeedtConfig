############ PLOT DATA #################
PlotData: StrackSpeedt01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_speed_t_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_speed_t_<CASE>.h5 (2:32) 1e-3 0
  # z data -  speed_t, m/s
  /speed_t  XsectionData/strack_speed_t_<CASE>.h5 (:,2:32,3) 1 0
End
  
PlotData: StrackSpeedt15Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_speed_t_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_speed_t_<CASE>.h5 (2:32) 1e-3 0
  # z data -  speed_t, m/s
  /speed_t  XsectionData/strack_speed_t_<CASE>.h5 (:,2:32,31) 1 0
End
  
PlotData: StrackSpeedt30Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_speed_t_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_speed_t_<CASE>.h5 (2:32) 1e-3 0
  # z data -  speed_t, m/s
  /speed_t  XsectionData/strack_speed_t_<CASE>.h5 (:,2:32,61) 1 0
End
  
PlotData: StrackSpeedt45Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_speed_t_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_speed_t_<CASE>.h5 (2:32) 1e-3 0
  # z data -  speed_t, m/s
  /speed_t  XsectionData/strack_speed_t_<CASE>.h5 (:,2:32,91) 1 0
End
  
PlotData: StrackSpeedt60Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_speed_t_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_speed_t_<CASE>.h5 (2:32) 1e-3 0
  # z data -  speed_t, m/s
  /speed_t  XsectionData/strack_speed_t_<CASE>.h5 (:,2:32,121) 1 0
End
  
############ PLOT AXES #################
PlotAxes: StrackSpeedtAxes 3
  14 2 [0.025_0.025]
  # x axis - radius
  Linear@Distance km 0 1650 linear [_] {_}
  # y axis - utc time
  Z km 0 6 linear [_] {_}
  # z axis - speed_t, m/s
  V_t m@s^-^1 -10 10 linear [_] {_}
End

