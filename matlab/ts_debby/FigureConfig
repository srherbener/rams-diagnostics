PlotData: MaxWindLine 2
  # x data - sim time in hours
  /t_coords DIAGS/storm_meas_<CASE>.h5 @ 2.7778e-4 -42
  # y data - 
  /max_wind DIAGS/storm_meas_<CASE>.h5 @ 1 0
End
  
PlotData: MinSlpLine 2
  # x data - sim time in hours
  /t_coords DIAGS/storm_meas_<CASE>.h5 @ 2.7778e-4 -42
  # y data - 
  /min_slp  DIAGS/storm_meas_<CASE>.h5 @ 1 0
End

