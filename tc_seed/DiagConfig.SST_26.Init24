#
# specs for extracting data via revu
#

AzavgDir: AzAveragedData
TsavgDir: TsAveragedData
PlotDir: plots
EofDir: EofData
DiagDir: DIAGS
FilterDir: FILTERS
ExpName: TC24
RamsDir: RAMS
RevuDir: HDF5
#ControlCase: TCS_CNTL
ControlCase: TCS_GN_C0100
UndefVal: -999
SstVal: 26

# cases (experiment names)
#  Case: <exp_name> <plot_name>
Case: TCS_CNTL      CONTROL
Case: TCS_GN_C0100  C0100
Case: TCS_GN_C0500  C0500
Case: TCS_GN_C1000  C1000
Case: TCS_GN_C2000  C2000
#Case: TCS_SD_C0100  SD_C0100
#Case: TCS_SD_C2000  SD_C2000

#Case: TCS_GN_C0050
#Case: TCS_GN_C0150
#Case: TCS_GN_C0200

# Time specs/directories
# TimeDir: <Dir> <TimeSpecForRevu>
TimeDir: NONE 1:150:1

# Variable specs
#  Var: <VarName> <RevuVarName> <TemplateSuffix>
Var: u u tcseed_3d
Var: v v tcseed_3d
Var: w w tcseed_3d

#Var: press press 3d
Var: dn0 dn0 tcseed_3d
Var: speed10m speed10m tcseed_h2d
Var: tempc tempc 3d
Var: sea_press sea_press tcseed_h2d
Var: theta_e theta_e tcseed_3d
Var: pcprate pcprate h2d

Var: cloud cloud 3d
Var: cloud_d cloud_diam 3d
Var: cloud_cm3 cloud_concen_cm3 3d
Var: drizzle drizzle 3d
Var: drizzle_d drizzle_diam 3d
Var: drizzle_cm3 drizzle_concen_cm3 3d
Var: rain rain 3d
Var: rain_d rain_diam 3d
Var: rain_m3 rain_concen_m3 3d
Var: liquid liquid 3d
Var: ice ice 3d
Var: vapor vapor 3d
Var: r_total r_total 3d

Var: ccn_conc ccn_concen 3d
Var: gccn_conc gccn_concen 3d
Var: ifn_conc ifn_conc 3d
Var: saltf_conc salt_film_concen 3d
Var: saltj_conc salt_jet_concen 3d

Var: vint_cloud vertint_cloud h2d
Var: vint_rain  vertint_rain  h2d
Var: vint_ice   vertint_ice   h2d

Var: lh_vapt latheatvapt 3d
Var: lh_frzt latheatfrzt 3d

#   AzavgEof: <name> <revu_file_prefix> <revu_var> <r1> <r2> <z1> <z2> <t1> <t2>
#
#    r1, r2, z1, z3 are in meters
#    t1, t2 are in seconds
#    these pick out a rectangular area in the azavg (x,z) slice
#    which is to be selected out before running the EOF analysis
AzavgEofConfig: 10 20

