############ PLOT DATA #################
PlotData: StrackDustPris01Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_pris_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_pris_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /dust_pris  XsectionData/strack_dust_pris_<CASE>.h5 (:,2:44,3) 1 0
End
  
PlotData: StrackDustPris15Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_pris_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_pris_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /dust_pris  XsectionData/strack_dust_pris_<CASE>.h5 (:,2:44,31) 1 0
End
  
PlotData: StrackDustPris30Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_pris_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_pris_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
  /dust_pris  XsectionData/strack_dust_pris_<CASE>.h5 (:,2:44,61) 1 0
End
  
PlotData: StrackDustPris45Contour 3
  # x data - linear distance, km
