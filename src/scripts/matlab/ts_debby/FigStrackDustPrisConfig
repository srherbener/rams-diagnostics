  # z data -  speed_t, m/s
  /dust_pris  XsectionData/strack_dust_pris_<CASE>.h5 (:,2:44,91) 1 0
End
  
PlotData: StrackDustPris60Contour 3
  # x data - linear distance, km
  /x_coords XsectionData/strack_dust_pris_<CASE>.h5 @ 1 0
  # y data - height, km
  /z_coords XsectionData/strack_dust_pris_<CASE>.h5 (2:44) 1e-3 0
  # z data -  speed_t, m/s
