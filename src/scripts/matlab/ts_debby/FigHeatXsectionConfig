############ PLOT DATA #################
# Units in files are K/30minutes. Multiply
# by 2 to convert to K/h
PlotData: LhfHeatXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhf_heat, K
  /all_ps_lhf_heat DIAGS/storm_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
PlotData: LhfHeatDiffXsectionPreSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_diff_xsections_<CASE>.h5 @ 1e-3 0
  # y data - height, km
  /z_coords DIAGS/storm_diff_xsections_<CASE>.h5 (1:44) 1e-3 0
  # z data -  lhf_heat, K
  /all_ps_lhf_heat_diff DIAGS/storm_diff_xsections_<CASE>.h5 (:,1:44) 2 0
End
  
PlotData: LhfHeatXsectionSalContour 3
  # x data - radius, km
  /x_coords DIAGS/storm_xsections_<CASE>.h5 @ 1e-3 0
